<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.first.studytest">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProgressBar.ProgressBarActivity" />
        <activity android:name=".ProgressBar.ProgressBarRoundActivity" />
        <activity android:name=".ProgressBar.ProgressBarNormalActivity" />
        <activity android:name=".ProgressBar.ProgressBarDIYActivity" />
        <activity android:name=".PullRefresh.NormalPullRefreshActivity" />
        <activity android:name=".RecyclerView.RecyclerViewActivity" />
        <activity android:name=".Notification.NotificationActivity" />
        <activity android:name=".ActionBar.ActionBarActivity" />
        <activity android:name=".ActionBar.NormalActionBarActivity" />
        <activity android:name=".ActionBar.ClickMenuActivity" />
        <activity android:name=".AlarmManager.AlarmManagerActivity" />
        <activity android:name=".BlueTooth.BlueToothMainActivity" />
        <activity android:name=".AlertDialog.AlertDialogMainActivity" />
        <activity android:name=".Intent.IntentMainActivity" />
        <activity android:name=".Intent.IntentNewActivity"></activity>
    </application>
</manifest>